    <title>Little House Creative | Gallery</title>
</head>

<!-- script for _portfolioItem -> image -> onError -->
<script>
    function removeElement(element) {
        element.remove();
    }
</script>

<body>
    <!-- Nav -->
    <nav class="navbar is-fixed-top" role="navigation" aria-label="main navigation">
        <div class="navbar-brand">
            <a class="navbar-item" href="/">
                <img src="./images/navbar-brand-house1.png" width="70">
                <span class="nav-text">Little House Creative</span>
            </a>
            <!-- Burger Menu -->
            <a role="button" class="navbar-burger" aria-label="menu" aria-expanded="false">
                <span aria-hidden="true"></span>
                <span aria-hidden="true"></span>
                <span aria-hidden="true"></span>
            </a>
            <!-- /Burger Menu -->
        </div>
        <div class="navbar-menu" id="navMenu">
            <div class="navbar-end">
                <a href="/portfolio" class="navbar-item is-tab">Portfolio</a>
                <a href="#" class="navbar-item is-tab">Services</a>
                <a href="#" class="navbar-item is-tab">About</a>

                <a href="/contact" class="navbar-item is-tab">Contact</a>
            </div>
        </div>
    </nav>
    <!-- /Nav -->
    <!-- Portfolio Section -->
    <section class="section">
        <div class="container" id="portfolio">
            <!--witchcraft to render gallery items from server-->
            <div class="columns is-variable is-1 is-multiline">
            <% for (var i = 0; i < portfolioItems.length; i++) { %>
                <% if (i % 3 == 0 && i != 0) { %>
                    </div>
                    <div class="columns is-variable is-1 is-multiline has-small-gap">
                <% } %>
                <%- include('_portfolioItem', {portfolioItem: portfolioItems[i], id: i}); %>
            <% } %>
            </div>

        </div>
    </section>
    <!-- /Portfolio Section -->
    <!-- Footer -->
    <footer class="footer animated fadeIn">
        <div class="container">
            <div class="columns">
                <div class="column">
                    <div class="center">
                        <a href="https://www.facebook.com/littlehousecreative" class="navbar-item"><i class="fab fa-facebook-f fa-2x"></i></a>
                        <a href="https://www.linkedin.com/in/nicole-maisonet-west-636bb9a0/" class="navbar-item"><i
                                class="fab fa-linkedin-in fa-2x"></i></a>
                        <a href="https://twitter.com/Nicole_LHC" class="navbar-item"><i class="fab fa-twitter fa-2x"></i></a>
                        <a href="https://www.pinterest.com/littlehousecreative/" class="navbar-item"><i class="fab fa-pinterest-p fa-2x"></i></a>
                        <a href="https://www.instagram.com/littlehousecreative/" class="navbar-item"><i class="fab fa-instagram fa-2x"></i></a>

                    </div>
                </div>
            </div>
        </div>
        <div class="content has-text-centered">
            <p>
                <strong>Little House Creative </strong>&COPY;<span id="year">
                    <script>
                        var d = new Date();
                        document.getElementById("year").innerHTML = d.getFullYear();
                    </script>
                </span>
            </p>
        </div>
        <div class="columns is-multiline has-text-centered">
            <div class="column is-full is-paddingless">
                <div class="center">
                    <span class="icon">
                        <i class="fas fa-phone"></i>
                    </span>
                    <p class="is-size-6 is-hidden-touch">407-796-1863</p>
                    <div></div>
                    <a href="tel:407-796-1863" class="is-hidden-widescreen is-hidden-tablet">407-796-1863</a>
                </div>
            </div>
        </div>
        <div class="columns has-text-centered is-multiline">
            <div class="column is-full is-paddingless">
                <div class="center">
                    <span class="icon">
                        <i class="fas fa-envelope"></i>
                    </span>
                    <a href="mailto:nicole@littlehousecreative.com" class="email-link">nicole@littlehousecreative.com</a>
                </div>
            </div>
        </div>
        <div class="columns has-text-centered is-multiline">
            <div class="column is-full is-paddingless">
                <div class="center">
                    <span class="icon">
                        <i class="fas fa-location-arrow"></i>
                    </span>
                    <p class="is-size-6">Orlando, FL</p>
                </div>
            </div>
        </div>
        <div class="columns">
            <div class="column has-text-centered center no-bottom-padding">
                <a href="#" class="navbar-item has-text-weight-bold">Sitemap</a>
                <a href="/privacy-policy" class="navbar-item has-text-weight-bold">Privacy Policy</a>
            </div>
        </div>
    </footer>
    <!-- /Footer -->
    <!-- JS Files -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

    <script src="./js/on_ready.min.js"></script>
    <!-- <script src="./js/scroll.js"></script> -->

    <!-- /JS Files -->

