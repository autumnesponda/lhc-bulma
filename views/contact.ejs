    <!-- Hero -->
    <section class="hero animated fadeIn is-medium">
        <div class="hero-body hero-image">
            <div class="container">
                <h1 class="title has-text-centered is-uppercase is-size-1 is-size-2-mobile has-text-white">
                    Contact Me
                </h1>
            </div>
        </div>
    </section>
    <!-- /Hero -->
    <section class="section animated fadeInUp faster">
        <div class="container">
            <h1 class="title has-text-centered has-text-weight-light">Let's Start Something Together!</h1>
            <div class="content has-text-centered">
                <p>I am always eager to hear from new clients wanting to find out more about my
                    services! Whether
                    you're curious about pricing, or you want more information about my services, or you know you want
                    me to help you design your dream space, I want to hear from you! So please, fill out the contact
                    form below and let's make something together!</p>
            </div>
            <div class="columns">
                <div class="column is-full">
                    <!-- Success Message -->
                    <% if (data.msg) { %>
                    <div class="notification is-success has-text-centered success-notification">
                        <button class="delete"></button>
                        <%= data.msg %>
                    </div>
                    <% } %>
                    <!-- /Success Message -->

                    <!-- Error Message -->
                    <% if (data.errors) { %>
                    <div class="notification is-danger has-text-centered error-notification">
                        <button class="delete"></button>
                        <%= data.errorMsg %>
                        <ul>
                            <% for(let i = 0; i < data.errors.length; i++) { %>
                            <li>
                                <%= data.errors[i].msg %>
                            </li>
                            <% } %>
                        </ul>
                    </div>
                    <% } %>
                    <!-- /Error Message -->

                    <form action="/send" method="POST">
                        <div class="field">
                            <label class="label">Full Name</label>
                            <div class="control has-icons-left">
                                <input class="input" type="text" placeholder="Name" name="name">
                                <span class="icon is-small is-left">
                                    <i class="fas fa-user"></i>
                                </span>
                            </div>
                            <p class="help is-danger name-invalid is-hidden">Name is required</p>
                        </div>

                        <div class="field">
                            <label class="label">Email</label>
                            <div class="control has-icons-left">
                                <input class="input email" type="email" placeholder="Email" name="email">
                                <span class="icon is-small is-left">
                                    <i class="fas fa-envelope"></i>
                                </span>
                            </div>
                        </div>

                        <div class="field">
                            <label class="label">Phone</label>
                            <div class="control has-icons-left">
                                <input class="input phone" type="tel" placeholder="Telephone" name="telephone">
                                <span class="icon is-small is-left">
                                    <i class="fas fa-phone"></i>
                                </span>
                            </div>
                        </div>

                        <div class="field">
                            <label class="label">Subject</label>
                            <div class="control">
                                <div class="select">
                                    <select class="subject" name="subject">
                                        <option value=""></option>
                                        <option value="consultations">Consultations</option>
                                        <option value="room-layout">Room Layout/Rearrangement</option>
                                        <option value="full-design">Full Design Package</option>
                                        <option value="design-build">Design Build Package</option>
                                        <option value="general">General Information</option>
                                        <option value="other">Other...</option>
                                    </select>
                                </div>
                            </div>
                        </div>


                        <div class="field">
                            <label class="label">Message</label>
                            <div class="control">
                                <textarea class="textarea" placeholder="Message" name="message"></textarea>
                            </div>
                        </div>

                        <div class="content">
                            <p class="is-size-7">Fields marked with an asterisk (*) are required</p>
                        </div>

                        <div class="field">
                            <div class="g-recaptcha" data-sitekey="6Lexu3MUAAAAALidxEDHb6xjZechtr3fWNeDaAMO"></div>
                        </div>

                        <div class="field">
                            <div class="control">
                                <button class="button submit" type="submit">Submit</button>
                                <button class="button" type="reset">Reset</button>
                            </div>
                        </div>
                </div>

                </form>
                <!-- <div class="column is-full-tablet is-half-desktop">
                    <div class="tile is-parent">
                        <article class="tile is-child notification">
                            <div class="content has-text-centered">
                                <span class="icon is-medium">
